{
    "name": "Fish Squad 4 Life",
    "version": "1.0",
    "description": "Add's a cool watermark to your browser",
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "main.html"
    },
    "content_scripts": [
	{
	    "matches": ["<all_urls>"],
	    "js":["content.js"]
	}
    ],
    "permissions": [
	"activeTab",
	"https://ajax.googleapis.com/"],
    "manifest_version": 2
}
